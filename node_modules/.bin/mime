#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/Entregable-Backend-Rios/node_modules/mime/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/Entregable-Backend-Rios/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/node_modules:/mnt/c/Users/USUARIO/Desktop/node_modules:/mnt/c/Users/USUARIO/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/Entregable-Backend-Rios/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/Entregable-Backend-Rios/node_modules/mime/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/Entregable-Backend-Rios/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/node_modules:/mnt/c/Users/USUARIO/Desktop/node_modules:/mnt/c/Users/USUARIO/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/USUARIO/Desktop/CARPETAS IMPORTANTES/Cursos/CURSO FULLSTACK/Curso backend/Entregable-Backend-Rios/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../mime/cli.js" "$@"
else
  exec node  "$basedir/../mime/cli.js" "$@"
fi
